<!DOCTYPE html>
<html>
<head>
    <!-- Required meta tags-->
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="au theme template">
    <meta name="author" content="Rajesh">
    <meta name="keywords" content="Expense Template">
    <title>Expense Demo</title>
    <link href="~/Content/Bootstrap4/css/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/Layout/css-hamburgers/hamburgers.min.css" rel="stylesheet" />
    <link href="~/Content/Layout/font-awesome-5/css/fontawesome-all.min.css" rel="stylesheet" />
    <link href="~/Content/Layout/perfect-scrollbar/perfect-scrollbar.css" rel="stylesheet" />
    <link href="~/Content/Layout/theme.css" rel="stylesheet" />
    <link href="~/Content/styles.css" rel="stylesheet" />
    <link href="~/Content/Site.css" rel="stylesheet" />
    <link href="~/Content/AssetsCSS/jqueryui.css" rel="stylesheet" />
    <link href="~/Content/AssetsCSS/Chart.min.css" rel="stylesheet" />

</head>
<body>
    <script>
        window.rootpath = "@(Url.Content("~"))";
    </script>
    <div class="page-wrapper">
        <!-- HEADER MOBILE-->
        <header class="header-mobile d-block d-lg-none">
            <div class="header-mobile__bar">
                <div class="container-fluid">
                    <div class="header-mobile-inner">
                        <div class="logo">
                            <label class="text-primary text-bold" style="font-size:22px">EXPENSE CALCULATOR</label>
                        </div>
                        <button class="hamburger hamburger--slider" type="button">
                            <span class="hamburger-box">
                                <span class="hamburger-inner"></span>
                            </span>
                        </button>
                    </div>
                </div>
            </div>
            <nav class="navbar-mobile">
                <div class="container-fluid">
                    <ul class="navbar-mobile__list list-unstyled ClientMenu">
                        <li class="btn_T_Dashboad"><a href="#"> <i class="fab fa-microsoft"></i>Dashboard</a></li>
                        <li class="btnAllTransactions"><a href="#"><i class="fas fa-chart-bar"></i>All Transactions</a></li>
                        <li class="has-sub">
                            <a class="js-arrow btnTenentDetails" href="#">
                                <i class="fas fa-table"></i>Tenent Details
                                <i class="fas fa-angle-down dashboardAngle" style="float: right;"></i>
                            </a>
                            <ul class="navbar-mobile-sub__list list-unstyled js-sub-list">
                                <li class="btnRentAgreement"> <a href="#">Rental Agreement</a> </li>
                                <li class="btnDocuments"> <a href="#">Documents</a> </li>
                            </ul>
                        </li>
                        <li class="btnHouseDetails"><a href="#"><i class="fas fa-home"></i>House Details </a></li>
                        <li class="btnEmcyContact"><a href="#"><i class="fas fa-calendar-alt"></i>Emergency Contact</a></li>
                        <li class="btnProximity"><a href="#"><i class="fas fa-map-marker-alt"></i>Proximity</a></li>
                        <li class="btnRaiseReq"><a href="#"><i class="far fa-comment"></i>Raise Request</a></li>
                    </ul>
                    <ul class="navbar-mobile__list list-unstyled AdminMenu">
                        <li class="btn_A_Dashboad"><a href="#"> <i class="fab fa-microsoft"></i>Dashboard</a></li>
                        <li class="btnAssetRegistration"><a href="#"><i class="fas fa-home"></i>Assets</a></li>
                        <li class="btnTenantAgreement"><a href="#"><i class="fas fa-home"></i>Tenant Agreement</a></li>
                        <li class="btn_A_AllTransactions"><a href="#"><i class="fas fa-chart-bar"></i>Transactions</a></li>
                        <li class="btn_A_Proximity"><a href="#"><i class="fas fa-map-marker-alt"></i>Proximities</a></li>
                        <li class="btn_A_EmcyContact"><a href="#"><i class="fas fa-calendar-alt"></i>Emergency Contacts</a></li>
                        
                        <li class="btn_A_SendMail"><a href="#"><i class="fas fa-envelope"></i>Send mail</a></li>
                        <li class="btn_A_ReqReceive"><a href="#"><i class="far fa-comments"></i>Requests</a></li>
                    </ul>
                </div>
            </nav>
        </header>
        <!-- END HEADER MOBILE-->
        <!-- MENU SIDEBAR-->
        <aside class="menu-sidebar d-none d-lg-block">
            <div class="logo"><label class="text-primary text-bold">EXPENSE CALCULATOR</label></div>
            <div class="menu-sidebar__content js-scrollbar1">
                <nav class="navbar-sidebar">
                    <ul class="list-unstyled navbar__list ClientMenu">
                        <li class="btn_T_Dashboad"><a href="#"> <i class="fab fa-microsoft"></i>Dashboard</a></li>
                        <li class="btnAllTransactions"> <a href="#"> <i class="fas fa-chart-bar"></i>All Transactions</a> </li>
                        <li class="has-sub">
                            <a class="js-arrow btnTenentDetails" href="#">
                                <i class="fas fa-table"></i>Tenant Details
                                <i class="fas fa-angle-down dashboardAngle" style="float: right;"></i>
                            </a>
                            <ul class="navbar-mobile-sub__list list-unstyled js-sub-list">
                                <li class="btnRentAgreement"> <a href="#">Rental Agreement</a> </li>
                                <li class="btnDocuments"> <a href="#">Documents</a> </li>
                            </ul>
                        </li>
                        <li class="btnHouseDetails"> <a href="#"> <i class="fas fa-home"></i>House Details </a> </li>
                        <li class="btnEmcyContact"> <a href="#"> <i class="fas fa-calendar-alt"></i>Emergency Contact </a> </li>
                        <li class="btnProximity"> <a href="#"> <i class="fas fa-map-marker-alt"></i>Proximity </a> </li>
                        <li class="btnRaiseReq"> <a href="#"> <i class="far fa-comment"></i>Raise Request </a> </li>
                    </ul>
                    <ul class="list-unstyled navbar__list AdminMenu">
                        <li class="btn_A_Dashboad"><a href="#"> <i class="fab fa-microsoft"></i>Dashboard</a></li>
                        <li class="btnAssetRegistration"><a href="#"><i class="fas fa-home"></i>Assets</a></li>
                        <li class="btnTenantAgreement"><a href="#"><i class="fas fa-home"></i>Tenant Agreement</a></li>
                        <li class="btn_A_AllTransactions"><a href="#"><i class="fas fa-chart-bar"></i>Transactions</a></li>
                        <li class="btn_A_Proximity"><a href="#"><i class="fas fa-map-marker-alt"></i>Proximities</a></li>
                        <li class="btn_A_EmcyContact"><a href="#"><i class="fas fa-calendar-alt"></i>Emergency Contacts</a></li>
                        
                        <li class="btn_A_SendMail"><a href="#"><i class="fas fa-envelope"></i>Send mail</a></li>
                        <li class="btn_A_ReqReceive"><a href="#"><i class="far fa-comments"></i>Requests</a></li>
                    </ul>
                </nav>
            </div>
        </aside>
        <!-- END MENU SIDEBAR-->
        <!-- PAGE CONTAINER-->
        <div class="page-container">
            <!-- HEADER DESKTOP-->
            <header class="header-desktop">
                <div class="section__content section__content--p30">
                    <div class="container-fluid">
                        <div class="header-wrap">
                            <form class="form-header">
                                <input class="au-input au-input--xl" type="text" name="search"
                                       placeholder="Search here..." />
                                <button class="au-btn--submit" type="submit">
                                    <i class="zmdi zmdi-search"></i>
                                </button>
                            </form>
                            <span>
                                <i class="fas fa-home fontSize_20 text-primary"></i>
                                <label class="font-weight-bold mb-2 mt-2 asset-title ml-1 text-primary" id="lblAssetName"></label>
                            </span>
                            <div class="header-button">
                                <div class="noti-wrap">
                                    <!-- message -->
                                    <div class="noti__item js-item-menu" id="notificationsTab">
                                        @*<i class="zmdi zmdi-comment-more"></i>*@
                                        <i class="far fa-bell text-primary"></i>
                                        <span class="quantity" id="Notiquantity">3</span>
                                        <div class="mess-dropdown js-dropdown">
                                            <div class="notifi__title font-weight-bold">
                                                <p id="NotiCount"></p>
                                            </div>
                                            <div id="listNotifications"></div>
                                            <div class="notifi__item">
                                                <div class="bg-c1 img-cir img-40">
                                                    <i class="zmdi zmdi-email-open"></i>
                                                </div>
                                                <div class="content">
                                                    <p>You got a email notification</p>
                                                    <span class="date">April 12, 2018 06:50</span>
                                                </div>
                                            </div>
                                            @*<div class="notifi__item">
                                                    <div class="bg-c2 img-cir img-40">
                                                        <i class="zmdi zmdi-account-box"></i>
                                                    </div>
                                                    <div class="content">
                                                        <p>Your account has been blocked</p>
                                                        <span class="date">April 12, 2018 06:50</span>
                                                    </div>
                                                </div>
                                                <div class="notifi__item">
                                                    <div class="bg-c3 img-cir img-40">
                                                        <i class="zmdi zmdi-file-text"></i>
                                                    </div>
                                                    <div class="content">
                                                        <p>You got a new file</p>
                                                        <span class="date">April 12, 2018 06:50</span>
                                                    </div>
                                                </div>*@
                                            <div class="notifi__footer text-center">
                                                <button id="btnAllNotifn" class="btn btn-info" onclick="getAllNotification()">All notifications</button>
                                            </div>
                                        </div>
                                    </div>

                                    <!-- Calculator -->
                                    <div class="noti__item js-item-menu">
                                        <i class="fas fa-calculator text-primary"></i>
                                        <div class="notifi-dropdown js-dropdown">
                                            <div class="notifi__title font-weight-bold">
                                                <p>Simple Calculator</p>
                                            </div>
                                            <div class="notifi__item">
                                                <div id="calculator">
                                                    <!-- Screen and clear key -->
                                                    <div class="top">
                                                        <div class="screen"></div>
                                                        <span class="clear">C</span>
                                                    </div>
                                                    <div class="keys">
                                                        <!-- operators and other keys -->
                                                        <span>7</span>
                                                        <span>8</span>
                                                        <span>9</span>
                                                        <span class="operator">+</span>
                                                        <span>4</span>
                                                        <span>5</span>
                                                        <span>6</span>
                                                        <span class="operator">-</span>
                                                        <span>1</span>
                                                        <span>2</span>
                                                        <span>3</span>
                                                        <span class="operator">÷</span>
                                                        <span>0</span>
                                                        <span>.</span>
                                                        <span class="eval">=</span>
                                                        <span class="operator">x</span>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <!-- Settings -->
                                    <div class="noti__item js-item-menu">
                                        <i class="fas fa-cog text-primary"></i>
                                        @*<span class="quantity">1</span>*@
                                        <div class="email-dropdown js-dropdown">
                                            <div class="email__title font-weight-bold">
                                                <p>Customize</p>
                                            </div>
                                            <div>Light Theme</div>
                                            <div>Dark Theme</div>

                                            <label>Logo Backgrounds</label>
                                            <div>Logo colors</div>
                                            <label>Navbar Backgrounds( the top header )</label>
                                            <div>Navbar colors</div>
                                            <label>Navbar Backgrounds( the top header )</label>
                                            <div>Navbar colors</div>

                                            <label>Sidebar</label>
                                            <div>colors</div>

                                            <label>Image Background</label>
                                            <div>Images</div>
                                            <label>Page Content</label>
                                            <div>Page Content colors</div>
                                            <div>Reset to Defaults</div>

                                            <div class="email__item">
                                                <div class="image img-cir img-40">
                                                    <img src="~/Images/avatar-06.jpg" alt="Cynthia Harvey" />
                                                </div>
                                                <div class="content">
                                                    <p>Meeting about new dashboard...</p>
                                                    <span>Cynthia Harvey, 3 min ago</span>
                                                </div>
                                            </div>
                                            <div class="email__item">
                                                <div class="image img-cir img-40">
                                                    <img src="~/Images/avatar-05.jpg" alt="Cynthia Harvey" />
                                                </div>
                                                <div class="content">
                                                    <p>Meeting about new dashboard...</p>
                                                    <span>Cynthia Harvey, Yesterday</span>
                                                </div>
                                            </div>
                                            <div class="email__item">
                                                <div class="image img-cir img-40">
                                                    <img src="~/Images/avatar-04.jpg" alt="Cynthia Harvey" />
                                                </div>
                                                <div class="content">
                                                    <p>Meeting about new dashboard...</p>
                                                    <span>Cynthia Harvey, April 12,,2018</span>
                                                </div>
                                            </div>
                                            <div class="email__footer"> <a href="#">See all emails</a> </div>
                                        </div>
                                    </div>
                                    <div class="noti__item js-item-menu">
                                        <i class="fas fa-compress text-primary"></i>
                                    </div>
                                </div>
                                <div class="account-wrap">
                                    <div class="account-item clearfix js-item-menu">
                                        <div class="image">
                                            <img src="~/Images/Profile.jpg" class="img-thumbnail rounded-circle" alt="John Doe" style="border:1px solid #007bff;" />
                                        </div>
                                        <div class="content">
                                            <p class="UserName font-weight-bold"></p>
                                        </div>
                                        <div class="account-dropdown js-dropdown">
                                            <div class="info clearfix">
                                                <div class="image">
                                                    <a href="#">
                                                        <img src="~/Images/Profile.jpg" class="img-fluid img-thumbnail" alt="Some Text" />
                                                    </a>
                                                </div>
                                                <div class="content">
                                                    <h5 class="name UserName"> <a href="#"></a> </h5>
                                                    <span class="email UserMail"></span>
                                                </div>
                                            </div>
                                            <div class="account-dropdown__body">
                                                <div class="account-dropdown__item">
                                                    <a href="#"> <i class="zmdi zmdi-account"></i>Account </a>
                                                </div>
                                                <div class="account-dropdown__item">
                                                    <a href="#"> <i class="zmdi zmdi-settings"></i>Setting </a>
                                                </div>
                                                <div class="account-dropdown__item">
                                                    <a href="#"> <i class="zmdi zmdi-money-box"></i>Billing </a>
                                                </div>
                                            </div>
                                            <div id="btnLogOut" class="account-dropdown__footer">
                                                <a href="#"> <i class="zmdi zmdi-power"></i>Log Out</a>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </header>
            <!-- HEADER DESKTOP-->
            <!-- MAIN CONTENT-->
            <div id="RenderContent" class="main-content">
                @RenderBody()
            </div>
            <!-- END MAIN CONTENT-->
            <!-- END PAGE CONTAINER-->
        </div>
    </div>
    <footer>
        <div>
            <div class="offset-md-3 col-md-9 text-center font-bold">
                <div>
                    <p>
                        Copyright © @DateTime.Now.Year Expense Calculator. All Rights Reserved.
                    </p>
                </div>
            </div>
        </div>
    </footer>
    <script src="~/Scripts/Jquery/jQuery.js"></script>
    @Scripts.Render("~/bundles/jqueryui")
    <script src="~/Content/Layout/font-awesome-5/js/fontawesome.js"></script>
    <script src="~/Scripts/Bootstrap/bootstrap.min.js"></script>
    <script src="~/Scripts/Layout/chartjs/Chart.bundle.min.js"></script>
    <script src="~/Scripts/Layout/perfect-scrollbar/perfect-scrollbar.min.js"></script>
    <script src="~/Scripts/Assets/Chart.bundle.min.js"></script>
    <script src="~/Scripts/Assets/Chart.min.js"></script>
    <script src="~/Scripts/Layout/LayoutMain.js"></script>
    <script src="~/Scripts/Assets/Calculator.js"></script>
    <script src="~/Scripts/RestAPI/ApiDirectory.js"></script>
    <script src="~/Scripts/RestAPI/ManageAjax.js"></script>
    <script src="~/Scripts/src/Tenent/AllTransactions.js"></script>
    <script src="~/Scripts/src/Admin/adminGlobal.js"></script>
    <script src="~/Scripts/src/GlobalConstants.js"></script>
    <script src="~/Scripts/src/Tenent/TenantGlobal.js"></script>
    <script src="~/Scripts/src/Tenent/Proximities.js"></script>
    <script src="~/Scripts/src/Tenent/houseDetails.js"></script>
    <script src="~/Scripts/src/Tenent/SendRequest.js"></script>
    <script src="~/Scripts/src/Tenent/Notification.js"></script>
    <script src="~/Scripts/src/SendMail.js"></script>
    <script src="~/Scripts/src/GlobalFunctions.js"></script>
    @Scripts.Render("~/bundles/Mainjs")
    @Scripts.Render("~/bundles/Assetjs")

    @RenderSection("scripts", required: false)
</body>
</html>
