        <!-- END MENU SIDEBAR-->
        <!-- PAGE CONTAINER-->
        <div class="page-container">

            <!-- HEADER DESKTOP-->
            <header class="header-desktop global-bg-primary">
                <div class="section__content section__content--p30">
                    <div class="container-fluid">
                        <div class="header-wrap">
                            <form class="form-header">
                                <input class="au-input au-input--xl" type="text" name="search"
                                       placeholder="Search here..." />
                                <button class="au-btn--submit global-bg-primary">
                                    <i class="zmdi zmdi-search"></i>
                                </button>
                            </form>
                            <span>
                                <i class="fas fa-home fontSize_20 text-light"></i>
                                <label class="font-weight-bold mb-2 mt-2 asset-title ml-1 text-light" id="lblAssetNamehdr"></label>
                            </span>
                            <div class="header-button">
                                <div class="noti-wrap">
                                    <!-- Requests -->
                                    <div class="noti__item js-item-menu" id="requestTab">
                                        <i class="far fa-comment text-light"></i>
                                        <span class="quantity d-none" id="ReqQuantity"></span>
                                        <div class="mess-dropdown js-dropdown">
                                            <div class="notifi__title font-weight-bold">
                                                <p id="RequestCount"></p>
                                            </div>
                                            <div id="listRequest"></div>
                                            @*<div class="notifi__item">
                                                <div class="bg-c1 img-cir img-40">
                                                    <i class="zmdi zmdi-email-open"></i>
                                                </div>
                                                <div class="content">
                                                    <p>You got a email notification</p>
                                                    <span class="date">April 12, 2018 06:50</span>
                                                </div>
                                            </div>*@
                                            <div class="notifi__footer text-center">
                                                <button id="btnAllNotifn" class="btn btn-info" onclick="getRaiseReqHTML()">All Reuests</button>
                                            </div>
                                        </div>
                                    </div>

                                    <!-- Notification -->
                                    <div class="noti__item js-item-menu" id="notificationsTab">
                                        @*<i class="zmdi zmdi-comment-more"></i>*@
                                        <i class="far fa-bell text-light"></i>
                                        <span class="quantity bg-light text-dark font-weight-bolder d-none" id="Notiquantity"></span>
                                        <div class="mess-dropdown js-dropdown">
                                            <div class="notifi__title font-weight-bold">
                                                <p id="NotiCount"></p>
                                            </div>
                                            <div id="listNotifications"></div>
                                            <div class="notifi__footer text-center">
                                                <button id="btnAllNotifn" class="btn btn-info" onclick="getAllNotification()">All notifications</button>
                                            </div>
                                        </div>
                                    </div>

                                    <!-- Calculator -->
                                    <div class="noti__item js-item-menu">
                                        <i class="fas fa-calculator text-light"></i>
                                        <div class="notifi-dropdown js-dropdown">
                                            <div class="notifi__title font-weight-bold">
                                                <p>Simple Calculator</p>
                                            </div>
                                            <div class="notifi__item">
                                                <div id="calculator">
                                                    <!-- Screen and clear key -->
                                                    <div class="top">
                                                        <div class="screen"></div>
                                                        <span class="clear">C</span>
                                                    </div>
                                                    <div class="keys">
                                                        <!-- operators and other keys -->
                                                        <span>7</span>
                                                        <span>8</span>
                                                        <span>9</span>
                                                        <span class="operator">+</span>
                                                        <span>4</span>
                                                        <span>5</span>
                                                        <span>6</span>
                                                        <span class="operator">-</span>
                                                        <span>1</span>
                                                        <span>2</span>
                                                        <span>3</span>
                                                        <span class="operator">÷</span>
                                                        <span>0</span>
                                                        <span>.</span>
                                                        <span class="eval">=</span>
                                                        <span class="operator">x</span>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <!-- Settings -->
                                    <div class="noti__item js-item-menu">
                                        <i class="fas fa-cog text-light"></i>
                                        @*<span class="quantity">1</span>*@
                                        <div class="email-dropdown js-dropdown pl-2 text-dark">
                                            <div style="overflow-y:scroll;height:580px">
                                                <div class="email__title font-weight-bold">
                                                    <p>Customize Panel</p>
                                                </div>
                                                <div class="h5">Theme</div>
                                                <div class="d-flex p-2 text-center">
                                                    <div class="flex-fill border-right">Light</div>
                                                    <div class="flex-fill border-right">
                                                        <label class="toggleSwitch">
                                                            <input type="checkbox" class="chkToggle" id="chktoggleSwitch">
                                                            <span class="slider round"></span>
                                                        </label>
                                                    </div>
                                                    <div class="flex-fill">Dark</div>
                                                </div>
                                                <div>
                                                    <div class="h5">Logo Backgrounds</div>
                                                    <span class="logoFill bg-light"></span>
                                                    <span class="logoFill global-bg-primary"></span>
                                                    <span class="logoFill bg-primary"></span>
                                                    <span class="logoFill bg-info"></span>
                                                    <span class="logoFill bg-warning"></span>
                                                    <span class="logoFill bg-danger"></span>
                                                    <span class="logoFill bg-success"></span>
                                                    <span class="logoFill bg-dark"></span>
                                                </div>
                                                <div class="h5">Navbar Backgrounds</div>
                                                <div>
                                                    <span class="logoFill bg-light" data-color="f8f9fa"></span>
                                                    <span class="logoFill global-bg-primary" data-color="fb8103"></span>
                                                    <span class="logoFill bg-primary" data-color="007bff"></span>
                                                    <span class="logoFill bg-info" data-color="17a2b8"></span>
                                                    <span class="logoFill bg-warning" data-color="ffc107"></span>
                                                    <span class="logoFill bg-danger" data-color="dc3545"></span>
                                                    <span class="logoFill bg-success" data-color="28a745"></span>
                                                    <span class="logoFill bg-dark" data-color="343a40"></span>
                                                </div>
                                                <div class="h5">Navbar Colors</div>
                                                <div>
                                                    <span class="logoFill bg-light"></span>
                                                    <span class="logoFill global-bg-primary"></span>
                                                    <span class="logoFill bg-primary"></span>
                                                    <span class="logoFill bg-info"></span>
                                                    <span class="logoFill bg-warning"></span>
                                                    <span class="logoFill bg-danger"></span>
                                                    <span class="logoFill bg-success"></span>
                                                    <span class="logoFill bg-dark"></span>
                                                </div>
                                                <div class="h5">Sidebar Background</div>
                                                <div>
                                                    <span class="logoFill bg-light"></span>
                                                    <span class="logoFill global-bg-primary"></span>
                                                    <span class="logoFill bg-primary"></span>
                                                    <span class="logoFill bg-info"></span>
                                                    <span class="logoFill bg-warning"></span>
                                                    <span class="logoFill bg-danger"></span>
                                                    <span class="logoFill bg-success"></span>
                                                    <span class="logoFill bg-dark"></span>
                                                </div>
                                                <div class="h5">Sidebar Colors</div>
                                                <div>
                                                    <span class="logoFill bg-light"></span>
                                                    <span class="logoFill global-bg-primary"></span>
                                                    <span class="logoFill bg-primary"></span>
                                                    <span class="logoFill bg-info"></span>
                                                    <span class="logoFill bg-warning"></span>
                                                    <span class="logoFill bg-danger"></span>
                                                    <span class="logoFill bg-success"></span>
                                                    <span class="logoFill bg-dark"></span>
                                                </div>

                                                <label>Image Background</label>
                                                <div>Images</div>
                                                <label>Page Content</label>
                                                <div>Page Content colors</div>
                                                <div>Reset to Defaults</div>

                                                <div class="email__item">
                                                    <div class="image img-cir img-40">
                                                        <img src="~/Images/avatar-06.jpg" alt="Cynthia Harvey" />
                                                    </div>
                                                    <div class="content">
                                                        <p>Meeting about new dashboard...</p>
                                                        <span>Cynthia Harvey, 3 min ago</span>
                                                    </div>
                                                </div>
                                                <div class="email__item">
                                                    <div class="image img-cir img-40">
                                                        <img src="~/Images/avatar-05.jpg" alt="Cynthia Harvey" />
                                                    </div>
                                                    <div class="content">
                                                        <p>Meeting about new dashboard...</p>
                                                        <span>Cynthia Harvey, Yesterday</span>
                                                    </div>
                                                </div>
                                                <div class="email__item">
                                                    <div class="image img-cir img-40">
                                                        <img src="~/Images/avatar-04.jpg" alt="Cynthia Harvey" />
                                                    </div>
                                                    <div class="content">
                                                        <p>Meeting about new dashboard...</p>
                                                        <span>Cynthia Harvey, April 12,,2018</span>
                                                    </div>
                                                </div>
                                                <div class="email__footer"> <a href="#">See all emails</a> </div>
                                            </div>

                                        </div>
                                    </div>
                                    <div class="noti__item js-item-menu" id="fullscreen">
                                        <i class="fas fa-compress text-light"></i>
                                    </div>
                                </div>
                                <div class="account-wrap">
                                    <div class="account-item clearfix js-item-menu">
                                        <div class="image">
                                            <img src="~/Images/Profile.jpg" class="img-thumbnail rounded-circle global-text-primary" alt="John Doe" style="border:1px solid;" />
                                        </div>
                                        <div class="content">
                                            <p class="UserName font-weight-bold"></p>
                                        </div>
                                        <div class="account-dropdown js-dropdown">
                                            <div class="info clearfix border shadow-sm">
                                                <div class="image">
                                                    <a href="#">
                                                        <img src="~/Images/Profile.jpg" class="img-fluid img-thumbnail" alt="Some Text" />
                                                    </a>
                                                </div>
                                                <div class="content">
                                                    <h5 class="name UserName"> <a href="#"></a> </h5>
                                                    <span class="email UserMail"></span>
                                                </div>
                                            </div>
                                            <div class="account-dropdown__body">
                                                <div class="account-dropdown__item">
                                                    <a href="#"> <i class="zmdi zmdi-account"></i>Account </a>
                                                </div>
                                                <div class="account-dropdown__item">
                                                    <a href="#"> <i class="zmdi zmdi-settings"></i>Setting </a>
                                                </div>
                                                <div class="account-dropdown__item btnEmcyContact" id="btnEmrcyContact">
                                                    <a href="#"> <i class="zmdi zmdi-money-box"></i>Contact Details </a>
                                                </div>
                                            </div>
                                            <div id="btnLogOut" class="account-dropdown__footer">
                                                <a href="#"> <i class="zmdi zmdi-power"></i>Log Out</a>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div id="forToast"></div>
            </header>
            <!-- HEADER DESKTOP-->
            <!-- MAIN CONTENT-->
            <div id="RenderContent" class="main-content">
                @RenderBody()
            </div>
            <!-- END MAIN CONTENT-->
            <!-- END PAGE CONTAINER-->
        </div>
        <div class="offset-md-3 col-md-9 text-center font-bold mt-2">
            <div><p>Copyright © @DateTime.Now.Year HOME SWEET HOME. All Rights Reserved.</p></div>
        </div>

        <!-- Modal -->
        <div class="modal fade" id="exampleModalLong" tabindex="-1" role="dialog" aria-labelledby="modalTitle" aria-hidden="true">
            <div class="modal-dialog" role="document">
                <div class="modal-content">
                    <div class="modal-header">
                        <h5 class="modal-title" id="modalTitle">Modal title</h5>
                        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                            <span aria-hidden="true">&times;</span>
                        </button>
                    </div>
                    <div class="modal-body" id="modalBody">teehee</div>
                    <div class="modal-footer">
                        <button type="button" class="btn btn-secondary" data-dismiss="modal">No</button>
                        <button type="button" class="btn btn-danger" >Yes</button>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <footer>
        
    </footer>
    <script src="~/Scripts/Jquery/jQuery.js"></script>
    @Scripts.Render("~/bundles/jqueryui")
    <script src="~/Content/Layout/font-awesome-5/js/fontawesome.js"></script>
    <script src="~/Scripts/Bootstrap/bootstrap.min.js"></script>
    <script src="~/Scripts/Assets/DataTables/datatables.min.js"></script>
    <script src="~/Scripts/Layout/chartjs/Chart.bundle.min.js"></script>
    <script src="~/Scripts/Layout/perfect-scrollbar/perfect-scrollbar.min.js"></script>
    <script src="~/Scripts/Assets/Chart.bundle.min.js"></script>
    <script src="~/Scripts/Assets/Chart.min.js"></script>
    <script src="~/Scripts/Layout/LayoutMain.js"></script>
    <script src="~/Scripts/Assets/Calculator.js"></script>
    <script src="~/Scripts/RestAPI/ApiDirectory.js"></script>
    <script src="~/Scripts/RestAPI/ManageAjax.js"></script>
    <script src="~/Scripts/src/Tenent/AllTransactions.js"></script>
    <script src="~/Scripts/src/GlobalConstants.js"></script>
    <script src="~/Scripts/src/Tenent/TenantGlobal.js"></script>
    <script src="~/Scripts/src/Tenent/Proximities.js"></script>
    <script src="~/Scripts/src/Tenent/houseDetails.js"></script>
    <script src="~/Scripts/src/Tenent/SendRequest.js"></script>
    <script src="~/Scripts/src/Tenent/Notification.js"></script>
    <script src="~/Scripts/src/SendMail.js"></script>
    <script src="~/Scripts/src/GlobalFunctions.js"></script>
    <script src="~/Scripts/src/Admin/PropertyTax.js"></script>
    <script src="~/Scripts/src/Admin/TenantAgreement.js"></script>
    <script src="~/Scripts/src/Admin/adminGlobal.js"></script>
    @Scripts.Render("~/bundles/Mainjs")
    @Scripts.Render("~/bundles/Assetjs")

    @RenderSection("scripts", required: false)
</body>
</html>
