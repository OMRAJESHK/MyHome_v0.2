    <div id="loginToast"><div class="fade border w-25" data-delay="2000" id="tstNotifyUser" style="position: absolute;top: 8rem;right: 20px;z-index: 1;">
    <div class="toast-body" id="toastBody"><div class="font-weight-bold">gfhfghfghf</div></div>
</div></div>

    @*@Html.Partial("_LoginPartial")*@
    @RenderBody()
