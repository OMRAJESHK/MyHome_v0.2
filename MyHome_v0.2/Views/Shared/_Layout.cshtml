    <script src="~/Content/LandingPage/jquery.easing/jquery.easing.min.js"></script>
    <script src="~/Content/LandingPage/owl.carousel/owl.carousel.min.js"></script>
    <script src="~/Content/LandingPage/aos/aos.js"></script>
    <script src="~/Scripts/LandingPage/main.js"></script>

